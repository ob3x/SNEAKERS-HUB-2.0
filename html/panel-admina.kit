<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Panel | SneakersHub.pl</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
        href="https://fonts.googleapis.com/css2?family=Ubuntu:ital,wght@0,300;0,400;0,500;0,700;1,300;1,400;1,500;1,700&display=swap"
        rel="stylesheet">
    <link rel="stylesheet" href="dist/css/style.min.css">
</head>

<body>
    <!-- @include 'nav' -->
    <header class="panel">
        <button class="panel-btn">
            <div class="panel-btn__bars"></div>
        </button>
        <div class="panel-left">
            <ul class="panel-left__list">
                <li class="panel-left__list-item"><a
                        class="panel-left__list-item-style panel-left__list-item-style--active"
                        href="panel-admina.html">
                        <div class="panel-left__list-item--icon panel-left__list-item--icon-house"></div> Pulpit
                    </a></li>
                <li class="panel-left__list-item"><a class="panel-left__list-item-style" href="zamowienia-admina.html">
                        <div class="panel-left__list-item--icon panel-left__list-item--icon-shoppingbag"></div>
                        Zamówienia
                    </a></li>
                <li class="panel-left__list-item"><a class="panel-left__list-item-style" href="konta-admina.html">
                        <div class="panel-left__list-item--icon panel-left__list-item--icon-account"></div>Konta
                    </a></li>
                <li class="panel-left__list-item"><a class="panel-left__list-item-style" href="index.html">
                        <div class="panel-left__list-item--icon panel-left__list-item--icon-arrow"></div>Wyloguj się
                    </a></li>
            </ul>
        </div>
        <div class="panel-right">
            <h2 class="panel-right__title">Pulpit</h2>
            <div class="admin-box">
                <div class="admin-code">
                    <p>DODAJ PRZEDMIOT</p>
                    <input type="file">
                    <input type="text" placeholder="firma">
                    <input type="text" placeholder="model">
                    <input type="text" placeholder="cena 'tylko liczba'">
                    <input type="text" placeholder="styl">
                    <p>Ma sie dodawac po 1 sztuce na rozmiar</p>
                    <input type="text" placeholder="rozmiary, po przecinku">
                    <button>DODAJ</button>
                </div>
                <div class="admin-code">
                    <p>USUN PRZEDMIOT</p>
                    <input type="text" placeholder="styl">
                    <button>USUN</button>
                </div>
                <div class="admin-code">
                    <p>USUN ROZMIAR</p>
                    <input type="text" placeholder="styl">
                    <input type="text" placeholder="rozmiar do usuniecia">
                    <button>USUN</button>
                </div>
                <div class="admin-code">
                    <p>DODAJ PROMOCJE</p>
                    <input type="text" placeholder="styl">
                    <input type="text" placeholder="% promocji">
                    <button>DODAJ</button>
                </div>
                <div class="admin-code">
                    <P>DODAJ KOD PROMOCYJNY</P>
                    <input type="text" class="admin-code-item">
                    <p class="admin-code__text">Liczba użyc</p>
                    <select name="count" id="">
                        <option value="1">1</option>
                        <option value="2">2</option>
                        <option value="3">3</option>
                        <option value="4">4</option>
                        <option value="5">5</option>
                        <option value="6">6</option>
                        <option value="7">7</option>
                        <option value="8">8</option>
                        <option value="9">9</option>
                        <option value="10">10</option>
                        <option value="11">nieskończoność</option>
                    </select>
                    <button class="admin__btn">DODAJ</button>
                </div>
            </div>
        </div>
    </header>
    <!-- @include 'footer' -->
    <script src="dist/js/panel.min.js"></script>
    <script src="dist/js/footer.min.js"></script>
    <script src="dist/js/account.min.js"></script>
    <script src="dist/js/index.min.js"></script>
</body>

</html>